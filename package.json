{
  "name": "startkit-typescript",
  "version": "0.0.1",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/startkit-dev/startkit-typescript#readme",
  "bugs": {
    "url": "https://github.com/startkit-dev/startkit-typescript/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/startkit-dev/startkit-typescript.git"
  },
  "license": "MIT",
  "author": "",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "startkit-typescript": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup lib/index.ts lib/cli.ts --dts --clean --format cjs,esm",
    "check": "concurrently bun:type-check bun:lint bun:format:check bun:test",
    "clean": "git clean -xdf dist",
    "dev": "tsx ./lib/cli.ts",
    "format": "prettier --write . --ignore-path .gitignore",
    "format:check": "prettier --check . --ignore-path .gitignore",
    "lint": "eslint lib --ext .ts",
    "outdated": "npx npm-check-updates --interactive --format group",
    "prepublish": "bun run build",
    "setup": "./bin/setup",
    "start": "node dist/cli.js",
    "test": "bun test",
    "type-check": "tsc --pretty --noEmit"
  },
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "@types/node": "^20.9.4",
    "@vercel/style-guide": "^5.1.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.1",
    "tsx": "^4.3.0",
    "typescript": "^5.3.2"
  }
}
