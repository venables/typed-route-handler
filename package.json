  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js",
    "client": "./dist/client.mjs"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "client"
  ],
  "scripts": {
    "build": "tsup lib/index.ts lib/client/index.ts --dts --clean --format cjs,esm",
    "check": "concurrently bun:type-check bun:lint bun:format:check bun:test",
    "clean": "git clean -xdf dist",
    "format": "prettier --write . --ignore-path .gitignore",
    "format:check": "prettier --check . --ignore-path .gitignore",
    "lint": "eslint lib --ext .ts",
    "outdated": "npx npm-check-updates --interactive --format group",
    "prepublish": "bun run build",
    "test": "bun test",
    "type-check": "tsc --pretty --noEmit"
  },
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "@types/node": "^20.9.4",
    "@vercel/style-guide": "^5.1.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "eslint-config-next": ">= 13",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.1",
    "tsx": "^4.3.0",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "next": ">= 13"
  }
